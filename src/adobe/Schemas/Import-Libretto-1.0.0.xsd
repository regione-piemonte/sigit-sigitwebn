<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.csi.it/sigit/sigitwebn/xml/importmassivo/libretto/data"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.csi.it/sigit/sigitwebn/xml/importmassivo/libretto/data"
		elementFormDefault="qualified"
		attributeFormDefault="unqualified">


	<xs:element name="libretto">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="libretto_catasto" minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="libretto_catasto">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="L1_schedaIdentificativa" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="L2_trattamentoAcqua" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="L4_generatori" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="L5_sistemiRegolazioneContabilizzazione" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="L6_sistemiDistribuzione" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="L7_sistemiEmissione" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="L8_sistemiAccumulo" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="L9_altriComponenti" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="L10_ventilazione" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="L14_consumi" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="L1_schedaIdentificativa">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="L1_1dataIntervento" type="xs:date" minOccurs="1" maxOccurs="1"/>
				<xs:element name="L1_1tipoIntervento" minOccurs="1" maxOccurs="1">
					<xs:simpleType>
						<xs:annotation>
							<xs:documentation>
								Valori ammessi:
								1-Nuova installazione
								2-Ristrutturazione
								3-Sostituzione generatore
								4-Compilazione libretto impianto esistente
							</xs:documentation>
						</xs:annotation>
						<xs:restriction base="xs:integer">
							<xs:enumeration value="1"/>
							<xs:enumeration value="2"/>
							<xs:enumeration value="3"/>
							<xs:enumeration value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<!-- I POD/PDR e rif catastali aggiuntivi sono da caricare a mano -->
				<xs:element name="L1_2flagUnitaImmSingle" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L1_2categoria" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:annotation>
							<xs:documentation>
								Valori ammessi:
								E.1
								E.2
								E.3
								E.4
								E.5
								E.6
								E.7
								E.8
							</xs:documentation>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="E.1"/>
							<xs:enumeration value="E.2"/>
							<xs:enumeration value="E.3"/>
							<xs:enumeration value="E.4"/>
							<xs:enumeration value="E.5"/>
							<xs:enumeration value="E.6"/>
							<xs:enumeration value="E.7"/>
							<xs:enumeration value="E.8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="L1_2volLordoRisc" type="decimaleType" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L1_2volLordoRaffr" type="decimaleType" minOccurs="0" maxOccurs="1" nillable="true"/>

				<xs:choice minOccurs="1" maxOccurs="unbounded"> 
					<xs:annotation>
						<xs:documentation>
							- Se valorizzate una delle voci viene impostato il check a true, di default a false
							- Se la stessa voce è present più volte (es L1_3servizioACS) viene tenuta in considerazione solo la prima
						</xs:documentation>
					</xs:annotation>
					<xs:element name="L1_3servizioACS" type="decimaleType" minOccurs="1" maxOccurs="1" nillable="true"/>
					<xs:element name="L1_3servizioClimaInv" type="decimaleType" minOccurs="1" maxOccurs="1" nillable="true"/>
					<xs:element name="L1_3servizioClimaEst" type="decimaleType" minOccurs="1" maxOccurs="1" nillable="true"/>
					<xs:element name="L1_3servizioAltro" minOccurs="1" maxOccurs="1" nillable="true">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:choice>

				<!--xs:choice minOccurs="1" maxOccurs="unbounded"-->
				<!--Almeno una delle voci L1_4xxx deve essere valorizzata a true-->
				<xs:element name="L1_4fluidoAcqua" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_4fluidoAria" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_4fluidoAltro" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<!--/xs:choice-->
				<!--xs:choice minOccurs="1" maxOccurs="unbounded"-->
				<!--Almeno una delle voci L1_5xxx deve essere valorizzata a true-->
				<xs:element name="L1_5tipoGeneratore" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoPompa" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoFrigo" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoTelerisc" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoTeleraff" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoCogen" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoAltro" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<!--/xs:choice-->

				<xs:element name="L1_5tipoIntegrazionePannelli" type="decimaleType6_2" minOccurs="0" maxOccurs="1" nillable="true"/>

				<xs:element name="L1_5tipoIntegrazioneAltro" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="descrizioneDecimaleType" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L1_5tipoPerClimaInv" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoPerClimaEst" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoPerACS" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L1_5tipoPerAltro" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>

				<!-- La sezione 1_6 e' compilata dal web -->
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="L2_trattamentoAcqua">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="L2_1contenutoAcqua" type="decimaleType8_3" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_2durezzaAcqua" type="decimaleType4_2" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_3trattAcquaAssente" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L2_3trattAcquaFiltrazione" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				<xs:element name="L2_3trattamentoAcquaAddolcimento" type="decimaleType4_2" minOccurs="0" maxOccurs="1" nillable="true">
					<!--xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="4"/>
							<xs:fractionDigits value="2"/>
							<xs:maxInclusive value="99.99"/>
						</xs:restriction>
					</xs:simpleType-->
				</xs:element>

				<xs:element name="L2_3trattAcquaChimico" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_3trattAcquaGeloAssente" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_3trattAcquaGeloEtilenico" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="percentoDecimaleType" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L2_3trattAcquaGeloPropilenico" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="percentoDecimaleType" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L2_4trattAcquaAcsAssente" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_4trattAcquaAcsFiltrazione" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L2_4trattamentoAcquaAcsAddolcimento"  type="decimaleType4_2" minOccurs="0" maxOccurs="1" nillable="true">
					<!--xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="4"/>
							<xs:fractionDigits value="2"/>
							<xs:maxInclusive value="99.99"/>
						</xs:restriction>
					</xs:simpleType-->
				</xs:element>
				<xs:element name="L2_4trattAcquaAcsChimico" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>

				<xs:element name="L2_5trattAcquaRaffreddamentoAssente" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2_5senzaRecuperoTermico" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5recuperoTermicoParziale" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5recuperoTermicoTotale" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2_5acquedotto" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5pozzo" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5acquaSuperficiale" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2_5filtrazioneSicurezza" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5filtrazioneMasse" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5filtrazioneNessunTrattamento" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5filtrazioneAltro" minOccurs="0" maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>


				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2_5trattAcquaAddolcimento" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5trattAcquaOsmosi" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5trattAcquaDemineraliz" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5trattAcquaNessunTrattamento" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5trattAcquaAltro" minOccurs="0" maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>

				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2_5condChimicoAnticrostante" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5condChimicoAnticorrosiva" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5condChimicoAnticrostanteAnticorrosiva" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5condChimicoBioacida" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5condChimicoNessunTrattamento" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
					<xs:element name="L2_5condChimicoAltro" minOccurs="0" maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:choice>
				<xs:element name="L2_5torreRaffreddamento" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L2_5conducibilita" type="decimaleType" minOccurs="0" maxOccurs="1" nillable="true"/>
							<xs:element name="L2_5taratura" type="decimaleType" minOccurs="0" maxOccurs="1" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="L4_generatori">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="L4_1GT" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_1numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_1gruppoTermico" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_1gruppoTermicoSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_2BR" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_2numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element name="L4_2GtCollegato" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_2bruciatore" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_2bruciatoreSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_3RC" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_3numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element name="L4_3GtCollegato" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_3recuperatore" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_3recuperatoreSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_4GF" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_4numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_4gruppoFrigo" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_4gruppoFrigoSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_5SC" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_5numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_5scambiatore" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_5scambiatoreSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_6CG" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_6numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_6cogeneratore" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_6cogeneratoreSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_7CS" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_7numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_7campoSolareTermico" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_7campoSolareTermicoSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L4_8AG" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L4_8numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_8altroGeneratore" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L4_8altroGeneratoreSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="L5_sistemiRegolazioneContabilizzazione">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="L5_1flagRegolazioneOnOff" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1flagRegolazioneCurvaIntegrata" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1flagRegolazioneCurvaIndipendente" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1SR" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L5_1numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L5_1sistemaRegolazione" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L5_1sistemaRegolazioneSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L5_1flagValvoleRegolazione" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1VR" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="L5_1numero" type="xs:integer" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L5_1valvolaRegolazione" minOccurs="1" maxOccurs="1"/>
							<xs:element ref="L5_1valvolaRegolazioneSostituito" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="L5_1flagSistemaRegolazioneMultigradino" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1flagSistemaRegolazioneInverter" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1flagAltroSistemaRegolazione" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="true"/>
				<xs:element name="L5_1descrizioneAltroSistemaRegolazione" minOccurs="0" maxOccurs="1" nillable="true">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="5000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L5_2termostatoControlloOnOff" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2termostatoControlloProporzionale" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2controlloEntalpico" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2controlloPortata" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
				</xs:choice>
				<!-- xs:choice minOccurs="1" maxOccurs="1">
					<xs:element name="L5_2valvoleTermostatichePresenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2valvoleTermostaticheAssenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
				</xs:choice>
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element name="L5_2valvole2ViePresenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2valvole2VieAssenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
				</xs:choice>
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element name="L5_2valvole3ViePresenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
					<xs:element name="L5_2valvole3VieAssenti" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
