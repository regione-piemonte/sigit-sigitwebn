              <xs:element name="L4_2dataDismiss" type="xs:date"/>
              <xs:element name="L4_2fabbricante" type="xs:string"/>
              <xs:element name="L4_2modello" type="xs:string"/>
              <xs:element name="L4_2matricola" type="xs:string"/>
              <xs:element name="L4_2tipologia" type="xs:string"/>
              <xs:element name="L4_2combustibile" type="xs:string"/>
              <xs:element name="L4_2portataTermMaxNom" type="xs:decimal"/>
              <xs:element name="L4_2portataTermMinNom" type="xs:decimal"/>

              <xs:element name="sezBRsostituite">
            		<xs:complexType>
            			<xs:sequence>
                               <xs:element ref="rowBRsost" minOccurs="0" maxOccurs="unbounded"/>
            			</xs:sequence>
            		</xs:complexType>
    	     </xs:element>

			</xs:sequence>
		</xs:complexType>
	</xs:element>

  	<xs:element name="rowBRsost">
		<xs:complexType>
			<xs:sequence>
              <xs:element name="flagRollBack4_2" type="xs:boolean"/> <!-- true / false -->
              <xs:element name="L4_2numGT" type="xs:integer"/>
              <xs:element name="L4_2dataInstallaz" type="xs:date"/>
              <xs:element name="L4_2dataDismiss" type="xs:date"/>
              <xs:element name="L4_2fabbricante" type="xs:string"/>
              <xs:element name="L4_2modello" type="xs:string"/>
              <xs:element name="L4_2matricola" type="xs:string"/>
              <xs:element name="L4_2tipologia" type="xs:string"/>
              <xs:element name="L4_2combustibile" type="xs:string"/>
              <xs:element name="L4_2portataTermMaxNom" type="xs:decimal"/>
              <xs:element name="L4_2portataTermMinNom" type="xs:decimal"/>
			</xs:sequence>
