<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.csi.it/sigit/sigitwebn/xml/libretto/data"
	       xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:catdig="http://www.csi.it/sigit/sigitwebn/xml/catdig/system"
           targetNamespace="http://www.csi.it/sigit/sigitwebn/xml/libretto/data"
           elementFormDefault="qualified" attributeFormDefault="unqualified">

 <xs:import namespace="http://www.csi.it/sigit/sigitwebn/xml/catdig/system" schemaLocation="../common/schemas/system-catdig-schema.xsd"/>

<xs:element name="MOD">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="Richiesta"/>
			<xs:element ref="catdig:system"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

  <xs:element name="Richiesta">
      <xs:complexType>
      <xs:sequence>
        <xs:element ref="datiPrecompilati"/>
        <xs:element ref="datiModulo"/>
        <xs:element ref="datiSchedaIdentificativaImp"/>
        <xs:element ref="datiSchedaTrattH2O"/>
        <xs:element ref="datiSchedaGT"/>
        <xs:element ref="datiSchedaBR"/>
        <xs:element ref="datiSchedaRC"/>
        <xs:element ref="datiSchedaGF"/>
        <xs:element ref="datiSchedaSC"/>
        <xs:element ref="datiSchedaCG"/>
        <xs:element ref="datiSchedaCS"/>
        <xs:element ref="datiSchedaAG"/>
        <xs:element ref="datiSchedaSistemiRegolaz"/>
        <xs:element ref="datiSchedaSistemiDistrib"/>
        <xs:element ref="datiSchedaSistemaEmissione"/>
        <xs:element ref="datiSchedaSistemaAC"/>
        <xs:element ref="datiAltriComponentiTE"/>
        <xs:element ref="datiAltriComponentiRV"/>
        <xs:element ref="datiAltriComponentiSC"/>
        <xs:element ref="datiAltriComponentiCI"/>
        <xs:element ref="datiAltriComponentiRC"/>
        <xs:element ref="datiAltriComponentiUT"/>
        <xs:element ref="datiVentilazMeccanicaVM"/>
        <xs:element ref="datiRisultatiGT"/>
        <xs:element ref="datiRisultatiGF"/>
        <xs:element ref="datiRisultatiSC"/>
        <xs:element ref="datiRisultatiCG"/>
        <xs:element ref="datiInterventiControllo"/>
        <xs:element ref="datiRisultatiIspez"/>
        <xs:element ref="datiConsumoCombu"/>
        <xs:element ref="datiConsumoEE"/>
        <xs:element ref="datiConsumoH2O"/>
        <xs:element ref="datiConsumoProdottiChimici"/>
        <!--
        -->
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- DATI precompilati da web -->
  <xs:element name="datiPrecompilati">
    <xs:complexType>
      <xs:sequence>
          <xs:element name="statoModulo" type="xs:string"/>  <!-- BOZZA; DEFINITIVO -->
          <xs:element name="codice_impianto" type="xs:string"/>
          <!-- intestazione modulo -->
          <xs:element name="codiceCatasto" type="xs:string"/>
          <!-- 1.2 UBICAZIONE E DESTINAZIONE DELL'EDIFICIO -->
          <xs:element name="L1_2indirizzo" type="xs:string"/>
          <xs:element name="L1_2civico" type="xs:string"/>
          <xs:element name="L1_2palazzo" type="xs:string"/>
          <xs:element name="L1_2scala" type="xs:string"/>
          <xs:element name="L1_2interno" type="xs:string"/>
          <xs:element name="L1_2comune" type="xs:string"/>
          <xs:element name="L1_2prov" type="xs:string"/>

          <xs:element name="sezCatasto">
    		<xs:complexType>
    			<xs:sequence>
                       <xs:element ref="rowCatasto" minOccurs="1" maxOccurs="unbounded"/>
    			</xs:sequence>
    		</xs:complexType>
	     </xs:element>

         <!-- 1.6 RESPONSABILE DELL'IMPIANTO -->
         <xs:element name="L1_6cognome" type="xs:string"/>
         <xs:element name="L1_6nome" type="xs:string"/>
         <xs:element name="L1_6cf" type="xs:string"/>
         <xs:element name="L1_6ragSociale" type="xs:string"/>
         <xs:element name="L1_6piva" type="xs:string"/>
         
          <!-- 3. NOMINA TERZO RESPONSABILE IMPIANTO -->
          <xs:element name="sezNomine">
    		<xs:complexType>
    			<xs:sequence>
                       <xs:element ref="rowNomine" minOccurs="1" maxOccurs="unbounded"/>
    			</xs:sequence>
    		</xs:complexType>
	     </xs:element>
	     
         <!-- precompilazione combo della sezione Generatori e sez 14-->
    	  <xs:element name="elencoFabbricante">
    			<xs:complexType>
    				<xs:sequence>
    					<xs:element name="fabbricante" minOccurs="1" maxOccurs="unbounded">
    						<xs:complexType>
    							<xs:sequence>
    			    				<xs:element name="codice" type="xs:string"/>
    			    				<xs:element name="descrizione" type="xs:string"/>
    							</xs:sequence>
    						</xs:complexType>
        				</xs:element>
    				</xs:sequence>
    			</xs:complexType>
    		</xs:element>

    	  <xs:element name="elencoCombustibile">
    			<xs:complexType>
    				<xs:sequence>
    					<xs:element name="combustibile" minOccurs="1" maxOccurs="unbounded">
    						<xs:complexType>
    							<xs:sequence>
    			    				<xs:element name="codice" type="xs:string"/>
    			    				<xs:element name="descrizione" type="xs:string"/>
    							</xs:sequence>
    						</xs:complexType>
        				</xs:element>
    				</xs:sequence>
    			</xs:complexType>
    		</xs:element>

    	  <xs:element name="elencoFluidoTermoVett">
    			<xs:complexType>
    				<xs:sequence>
    					<xs:element name="fluidoTermoVett" minOccurs="1" maxOccurs="unbounded">
    						<xs:complexType>
    							<xs:sequence>
    			    				<xs:element name="codice" type="xs:string"/>
    			    				<xs:element name="descrizione" type="xs:string"/>
    							</xs:sequence>
    						</xs:complexType>
        				</xs:element>
    				</xs:sequence>
    			</xs:complexType>
    		</xs:element>
    		
             <!-- precompilazione combo unita' misura -->
        	  <xs:element name="elencoUM">
        			<xs:complexType>
        				<xs:sequence>
        					<xs:element name="unitaMisura" minOccurs="1" maxOccurs="unbounded">
        						<xs:complexType>
        							<xs:sequence>
        			    				<xs:element name="codice" type="xs:string"/>
        			    				<xs:element name="descrizione" type="xs:string"/>
        							</xs:sequence>
        						</xs:complexType>
            				</xs:element>
        				</xs:sequence>
        			</xs:complexType>
        		</xs:element>
	     
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="rowCatasto">
		<xs:complexType>
			<xs:sequence>
              <xs:element name="L1_2sezione" type="xs:string"/>
              <xs:element name="L1_2foglio" type="xs:string"/>
              <xs:element name="L1_2particella" type="xs:string"/>
              <xs:element name="L1_2sub" type="xs:string"/>
              <xs:element name="L1_2pod" type="xs:string"/>
              <xs:element name="L1_2pdr" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
   </xs:element>
   
   <xs:element name="rowNomine">
		<xs:complexType>
			<xs:sequence>
              <xs:element name="L3cognome" type="xs:string"/>
              <xs:element name="L3nome" type="xs:string"/>
              <xs:element name="L3codiceFiscale" type="xs:string"/>
              <xs:element name="L3ragioneSociale" type="xs:string"/>
              <xs:element name="L3piva" type="xs:string"/>
              <xs:element name="L3flagProprietario" type="xs:boolean"/>
              <xs:element name="L3flagAmministratore" type="xs:boolean"/>
              <xs:element name="L3ragSocialeDitta" type="xs:string"/>
              <xs:element name="L3cciaa" type="xs:string"/>
              <xs:element name="L3dataValiditaContrattoDal" type="xs:date"/>
              <xs:element name="L3dataValiditaContrattoAl" type="xs:date"/>

			</xs:sequence>
		</xs:complexType>
	</xs:element>
  
  
  <!-- DATI FUNZIONALI AL COMPORTAMENTO DEL MODULO -->
  <xs:element name="datiModulo">
    <xs:complexType>
      <xs:sequence>
          <!--
          <xs:element name="flagPrecompilazione" type="xs:boolean"/>
          -->
          <xs:element name="flag_controllo_bozza" type="xs:boolean"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!-- 1. SCHEDA IDENTIFICATIVA DELL'IMPIANTO -->
  <xs:element name="datiSchedaIdentificativaImp">
    <xs:complexType>
      <xs:sequence>
         <!-- 1.1 TIPOLOGIA INTERVENTO -->
         <xs:element name="L1_1dataIntervento" type="xs:date"/>
         <xs:element name="L1_1flagNuovaInst" type="xs:boolean"/>
         <xs:element name="L1_1flagRistrutt" type="xs:boolean"/>
         <xs:element name="L1_1flagSostGenerat" type="xs:boolean"/>
         <xs:element name="L1_1flagCompilaLibretto" type="xs:boolean"/>
          
	     <xs:element name="L1_2flagUnitaImmSingle" type="xs:boolean"/>
         <xs:element name="L1_2flagE1" type="xs:boolean"/>
         <xs:element name="L1_2flagE2" type="xs:boolean"/>
         <xs:element name="L1_2flagE3" type="xs:boolean"/>
         <xs:element name="L1_2flagE4" type="xs:boolean"/>
         <xs:element name="L1_2flagE5" type="xs:boolean"/>
         <xs:element name="L1_2flagE6" type="xs:boolean"/>
         <xs:element name="L1_2flagE7" type="xs:boolean"/>
         <xs:element name="L1_2flagE8" type="xs:boolean"/>
         <xs:element name="L1_2volLordoRisc" type="xs:decimal"/>
         <xs:element name="L1_2volLordoRaffr" type="xs:decimal"/>
         
         <!-- 1.3 IMPIANTO TERMICO DESTINATO A SODDISFARE I SEGUENTI SERVIZI -->
         <xs:element name="L1_3flagProdACS" type="xs:boolean"/>
         <xs:element name="L1_3potUtileACS" type="xs:decimal"/>
         <xs:element name="L1_3flagClimaInv" type="xs:boolean"/>
         <xs:element name="L1_3potUtileClimaInv" type="xs:decimal"/>
         <xs:element name="L1_3flagClimaEst" type="xs:boolean"/>
         <xs:element name="L1_3potUtileClimaEst" type="xs:decimal"/>
         <xs:element name="L1_3flagAltro" type="xs:boolean"/>
         <xs:element name="L1_3descrAltro" type="xs:string"/>
         
         <!-- 1.4 TIPOLOGIA FLUIDO VETTORE -->
         <xs:element name="L1_4flagH2O" type="xs:boolean"/>
         <xs:element name="L1_4flagAria" type="xs:boolean"/>
         <xs:element name="L1_4flagAltro" type="xs:boolean"/>
         <xs:element name="L1_4descrAltro" type="xs:string"/>
          
         <!-- 1.5 INDIVIDUAZIONE DELLA TIPOLOGIA DEI GENERATORI -->
         <xs:element name="L1_5flagGeneratCombu" type="xs:boolean"/>
         <xs:element name="L1_5flagPompaCal" type="xs:boolean"/>
         <xs:element name="L1_5flagMaccFrigo" type="xs:boolean"/>
         <xs:element name="L1_5flagTelerisc" type="xs:boolean"/>
         <xs:element name="L1_5flagTeleraffr" type="xs:boolean"/>
         <xs:element name="L1_5flagCogener" type="xs:boolean"/>
         <xs:element name="L1_5flagAltro" type="xs:boolean"/>
         <xs:element name="L1_5descrAltro" type="xs:string"/>
         
         <xs:element name="L1_5flagPannelliSol" type="xs:boolean"/>
         <xs:element name="L1_5superfLordaTot" type="xs:decimal"/>
         <xs:element name="L1_5flagAltraIntegraz" type="xs:boolean"/>
         <xs:element name="L1_5descrAltrIntegraz" type="xs:string"/>
         <xs:element name="L1_5potUtile" type="xs:decimal"/>
         <xs:element name="L1_5flagClimaInv" type="xs:boolean"/>
         <xs:element name="L1_5flagClimaEst" type="xs:boolean"/>
         <xs:element name="L1_5flagProdACS" type="xs:boolean"/>
         <xs:element name="L1_5flagAltroPer" type="xs:boolean"/>
         <xs:element name="L1_5descrAltroPer" type="xs:string"/>
         
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!-- 2. TRATTAMENTO ACQUA -->
  <xs:element name="datiSchedaTrattH2O">
    <xs:complexType>
      <xs:sequence>
          <!-- 2.1 CONTENUTO D'ACQUA DELL'IMPIANTO DI CLIMATIZZAZIONE -->
          <xs:element name="L2_1contenutoH2OimpClima" type="xs:decimal"/>
          <!-- 2.2 DUREZZA TOTALE DELL'ACQUA -->
          <xs:element name="L2_2durezzaTotaleH2O" type="xs:decimal"/>
          <!-- 2.3 TRATTAMENTO DELL'ACQUA DELL'IMPIANTO DI CLIMATIZZAZIONE (Rif. UNI 8065) -->
          <xs:element name="L2_3flagAssenteH2Oclima" type="xs:boolean"/>
          <xs:element name="L2_3flagFiltrazione" type="xs:boolean"/>
          <xs:element name="L2_3flagAddolcimento" type="xs:boolean"/>
          <xs:element name="L2_3durezzaTotaleH2O" type="xs:decimal"/>
          <xs:element name="L2_3flagCondizChimico" type="xs:boolean"/>
          <xs:element name="L2_3flagAssenteProtGelo" type="xs:boolean"/>
          <xs:element name="L2_3flagGlicoleEtilenic" type="xs:boolean"/>
          <xs:element name="L2_3percConcentrazEtilenic" type="xs:decimal"/>
          <xs:element name="L2_3PHconcentrazEtilenic" type="xs:decimal"/>
          <xs:element name="L2_3flagGlicolePropLenic" type="xs:boolean"/>
          <xs:element name="L2_3percConcentrazPropLenic" type="xs:decimal"/>
          <xs:element name="L2_3PHconcentrazPropLenic" type="xs:decimal"/>
         <!-- 2.4 TRATTAMENTO DELL'ACQUA CALDA SANITARIA ..... -->
          <xs:element name="L2_4flagAssente" type="xs:boolean"/>
          <xs:element name="L2_4flagFiltrazione" type="xs:boolean"/>
          <xs:element name="L2_4flagAddolcimento" type="xs:boolean"/>
          <xs:element name="L2_4durezzaTotaleAddolcit" type="xs:decimal"/>
          <xs:element name="L2_4flagCondizChimico" type="xs:boolean"/>

         <!-- 2.5 TRATTAMENTO DELL'ACQUA DI RAFFREDDAMENTO DELL'IMPIANTO..... -->
          <xs:element name="L2_5flagAssente" type="xs:boolean"/>
          <xs:element name="L2_5flagSenzaRecupTerm" type="xs:boolean"/>
          <xs:element name="L2_5flagRecupTermParz" type="xs:boolean"/>
          <xs:element name="L2_5flagRecupTermTot" type="xs:boolean"/>
          <xs:element name="L2_5flagAcquedotto" type="xs:boolean"/>
          <xs:element name="L2_5flagPozzo" type="xs:boolean"/>
          <xs:element name="L2_5flagAcquaSuperficiale" type="xs:boolean"/>
          
          <xs:element name="L2_5flagFiltrazione" type="xs:boolean"/>
          <xs:element name="L2_5flagFiltrazSicurezza" type="xs:boolean"/>
          <xs:element name="L2_5flagFiltrazMasse" type="xs:boolean"/>
          <xs:element name="L2_5flagFiltrazAltro" type="xs:boolean"/>
          <xs:element name="L2_5filtrazioneAltro" type="xs:string"/>
          <xs:element name="L2_5flagFiltrazNoTrattam" type="xs:boolean"/>
          
          <xs:element name="L2_5flagTrattamentoH2O" type="xs:boolean"/>
          <xs:element name="L2_5flagTrattAddolcim" type="xs:boolean"/>
          <xs:element name="L2_5flagTrattOsmosi" type="xs:boolean"/>
          <xs:element name="L2_5flagTrattDemineralizz" type="xs:boolean"/>
          <xs:element name="L2_5flagTrattAltro" type="xs:boolean"/>
          <xs:element name="L2_5trattamentoAltro" type="xs:string"/>
          <xs:element name="L2_5flagTrattNoTrattam" type="xs:boolean"/>
          
          <xs:element name="L2_5flagCondizChimico" type="xs:boolean"/>
          <xs:element name="L2_5flagCondChimAnticrosta" type="xs:boolean"/>
          <xs:element name="L2_5flagCondChimAnticorr" type="xs:boolean"/>
          <xs:element name="L2_5flagCondChimAnticrostaAntiCorr" type="xs:boolean"/>
          <xs:element name="L2_5flagCondChimBiocida" type="xs:boolean"/>
          <xs:element name="L2_5flagCondChimAltro" type="xs:boolean"/>
          <xs:element name="L2_5condChimAltro" type="xs:string"/>
          <xs:element name="L2_5flagCondChimNoTrattam" type="xs:boolean"/>
          
          <xs:element name="L2_5flagPrefSpurgoAutom" type="xs:boolean"/>
          <xs:element name="L2_5conducibH2Oingresso" type="xs:decimal"/>
          <xs:element name="L2_5taraturaSpurgo" type="xs:decimal"/>

      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!-- 4.1 GENERATORI - GT -->
  <xs:element name="datiSchedaGT">
    <xs:complexType>
      <xs:sequence>
          
          <xs:element name="sezGruppiTermici">
    		<xs:complexType>
    			<xs:sequence>
                       <xs:element ref="rowGT" minOccurs="1" maxOccurs="unbounded"/>
    			</xs:sequence>
    		</xs:complexType>
	     </xs:element>

      </xs:sequence>
    </xs:complexType>
  </xs:element>


   <xs:element name="rowGT">
		<xs:complexType>
			<xs:sequence>
			  <xs:element name="L4_1dataControllo" type="xs:date"/>
              <xs:element name="flagDismissione" type="xs:boolean"/> <!-- 1 (il componente e' dismesso) / 0 -->
              <xs:element name="flagAnnullaDismissione" type="xs:boolean"/> <!-- 1 (posso annullare la dismissione) / 0 -->
              <xs:element name="flagGiaPresenteDB" type="xs:boolean"/> <!-- true (era gia' nel db) / false (e' nuovo) -->
              <xs:element name="L4_1numGT" type="xs:integer"/>
              <xs:element name="L4_1dataInstallaz" type="xs:date"/>
              <xs:element name="L4_1dataDismiss" type="xs:date"/>
              <xs:element name="L4_1fabbricante" type="xs:string"/>
              <xs:element name="L4_1modello" type="xs:string"/>
              <xs:element name="L4_1matricola" type="xs:string"/>
              <xs:element name="L4_1combustibile" type="xs:string"/>
              <xs:element name="L4_1fluidoTermoVett" type="xs:string"/>
              <xs:element name="L4_1potTermUtileMax" type="xs:decimal"/>
              <xs:element name="L4_1percRendimTermUtileMax" type="xs:decimal"/>
              <xs:element name="L4_1GTsingolo" type="xs:boolean"/>
              <xs:element name="L4_1GTModulare" type="xs:boolean"/>
              <xs:element name="L4_1numAnalisiFumi" type="xs:integer"/>
              <xs:element name="L4_1tupoRadiante" type="xs:boolean"/>
              <xs:element name="L4_1GenAriaCalda" type="xs:boolean"/>

              <xs:element name="sezGTsostituite">
            		<xs:complexType>
            			<xs:sequence>
                               <xs:element ref="rowGTsost" minOccurs="0" maxOccurs="unbounded"/>
            			</xs:sequence>
            		</xs:complexType>
    	     </xs:element>

			</xs:sequence>
		</xs:complexType>
	</xs:element>

  	<xs:element name="rowGTsost">
		<xs:complexType>
			<xs:sequence>
			  <xs:element name="L4_1dataControllo" type="xs:date"/>
              <xs:element name="flagRollBack4_1" type="xs:boolean"/> <!-- true (non posso fare rollBack) / false -->
              <xs:element name="L4_1dataInstallaz" type="xs:date"/>
              <xs:element name="L4_1dataDismiss" type="xs:date"/>
              <xs:element name="L4_1fabbricante" type="xs:string"/>
              <xs:element name="L4_1modello" type="xs:string"/>
              <xs:element name="L4_1matricola" type="xs:string"/>
              <xs:element name="L4_1combustibile" type="xs:string"/>
              <xs:element name="L4_1fluidoTermoVett" type="xs:string"/>
              <xs:element name="L4_1potTermUtileMax" type="xs:decimal"/>
              <xs:element name="L4_1percRendimTermUtileMax" type="xs:decimal"/>
              <xs:element name="L4_1GTsingolo" type="xs:boolean"/>
              <xs:element name="L4_1GTModulare" type="xs:boolean"/>
              <xs:element name="L4_1numAnalisiFumi" type="xs:integer"/>
              <xs:element name="L4_1tupoRadiante" type="xs:boolean"/>
              <xs:element name="L4_1GenAriaCalda" type="xs:boolean"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

  <!-- 4.2 BRUCIATORI -->
  <xs:element name="datiSchedaBR">
    <xs:complexType>
      <xs:sequence>

          <xs:element name="sezBR">
    		<xs:complexType>
    			<xs:sequence>
                       <xs:element ref="rowBR" minOccurs="1" maxOccurs="unbounded"/>
    			</xs:sequence>
    		</xs:complexType>
	     </xs:element>

      </xs:sequence>
    </xs:complexType>
  </xs:element>


   <xs:element name="rowBR">
		<xs:complexType>
			<xs:sequence>
              <xs:element name="flagDismissione" type="xs:boolean"/> <!-- 1 (il componente e' dismesso) / 0 -->
              <xs:element name="flagAnnullaDismissione" type="xs:boolean"/> <!-- 1 (posso annullare la dismissione) / 0 -->
              <xs:element name="L4_2numBR" type="xs:integer"/>
              <xs:element name="L4_2numGT" type="xs:integer"/>
